<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/fanh"
        android:layout_width="35dp"
        android:layout_height="25dp"
        android:background="@drawable/fanh"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="10dp"/>

    <androidx.appcompat.widget.SearchView
        android:id="@+id/com_search"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@id/fanh"
        android:background="@drawable/gwc_bg" />

    <Button
        android:id="@+id/yes"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="13dp"
        android:layout_marginRight="10dp"
        android:layout_toRightOf="@id/com_search"
        android:background="@color/red"
        android:text="确定" />
    <TextView
        android:id="@+id/lissous"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text=" 历史搜索 "
        android:layout_below="@id/com_search"
        android:layout_margin="10dp"
        />
    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@id/com_search"
        android:layout_alignParentRight="true"
        android:background="@drawable/delete"
        android:layout_marginTop="10dp"
        android:layout_marginRight="30dp"/>
    <TextView
        android:id="@+id/lis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="佳能EOS77d"
        android:layout_marginLeft="30dp"
        android:layout_below="@id/lissous"
        />
    <TextView
        android:id="@+id/sousfax"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text=" 搜索发现 "
        android:layout_below="@id/lis"
        android:layout_margin="10dp"
        />
    <TextView
        android:id="@+id/sous"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="佳能EOS80d"
        android:layout_marginLeft="30dp"
        android:layout_below="@id/sousfax"
        />
    <ImageView
        android:id="@+id/res"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/res"
        android:layout_below="@+id/sous"
        android:layout_margin="10dp"
        />
    <TextView
        android:id="@+id/res2"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="淘宝热搜"
        android:gravity="center"
        android:layout_toRightOf="@+id/res"
        android:layout_alignTop="@+id/res"
        />
    <TextView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:text="今日热搜全知道"
        android:gravity="center"
        android:layout_toRightOf="@+id/res2"
        android:layout_alignTop="@+id/res2"
        android:layout_alignParentRight="true"
        />
    <RelativeLayout
        android:id="@+id/rs1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/res">
        <ImageView
            android:id="@id/img1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/one"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img1"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs1">
        <ImageView
            android:id="@id/img2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/two"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img2"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs2">
        <ImageView
            android:id="@id/img3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/three"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img3"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs3">
        <ImageView
            android:id="@id/img4"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/four"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img4"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs4">
        <ImageView
            android:id="@id/img5"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/five"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img5"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs5">
        <ImageView
            android:id="@id/img6"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/six"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img6"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs6">
        <ImageView
            android:id="@id/img7"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/seven"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img7"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs7">
        <ImageView
            android:id="@id/img8"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="10dp"
            android:src="@drawable/eight"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img8"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_margin="10dp"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rs9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rs8">
        <ImageView
            android:id="@+id/img9"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/nine"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_toRightOf="@id/img9"
            android:text="双十二淘宝好货"
            android:gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            />
    </RelativeLayout>
</RelativeLayout>